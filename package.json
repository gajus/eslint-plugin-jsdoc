{
  "author": {
    "email": "gajus@gajus.com",
    "name": "Gajus Kuizinas",
    "url": "http://gajus.com"
  },
  "dependencies": {
    "comment-parser": "^0.5.4",
    "jsdoctypeparser": "^2.0.0-alpha-8",
    "lodash": "^4.17.11"
  },
  "description": "JSDoc linting rules for ESLint.",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/node": "^7.2.2",
    "@babel/plugin-transform-flow-strip-types": "^7.2.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@typescript-eslint/parser": "^1.5.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "eslint-config-canonical": "^16.1.0",
    "gitdown": "^2.5.7",
    "glob": "^7.1.3",
    "globby": "^9.0.0",
    "husky": "^1.3.1",
    "marked": "^0.6.1",
    "mocha": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "semantic-release": "^15.13.3",
    "typescript": "^3.3.4000"
  },
  "engines": {
    "node": ">=4"
  },
  "husky": {
    "hooks": {
      "post-commit": "npx run-s create-readme add-assertions commit-docs",
      "pre-commit": "npx run-s lint test build"
    }
  },
  "keywords": [
    "eslint",
    "plugin",
    "jsdoc"
  ],
  "license": "BSD-3-Clause",
  "main": "./dist/index.js",
  "name": "eslint-plugin-jsdoc",
  "peerDependencies": {
    "eslint": ">=4.14.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gajus/eslint-plugin-jsdoc"
  },
  "scripts": {
    "add-assertions": "babel-node ./src/bin/readme-assertions",
    "build": "run-s clear-dist transpile",
    "clear-dist": "rimraf ./dist",
    "commit-docs": "git commit -m \"docs: generate docs\" --no-verify README.md",
    "create-readme": "gitdown ./.README/README.md --output-file ./README.md",
    "lint": "eslint ./src ./test",
    "test": "mocha --recursive --require @babel/register",
    "transpile": "cross-env NODE_ENV=production babel ./src --out-dir ./dist --copy-files --source-maps"
  },
  "version": "1.0.0"
}
